<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
             <block class="Magento\Framework\View\Element\Html\Link" name="bss.faqs.header" before="-">
                <arguments>
                    <argument name="label" xsi:type="string" translate="false">FAQs</argument>
                    <argument name="path" xsi:type="string" translate="false">bss_faqs</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="footer_links">
            <block class="Magento\Framework\View\Element\Html\Link" name="bss.faqs.footer">
                <arguments>
                    <argument name="label" xsi:type="string">FAQs</argument>
                    <argument name="path" xsi:type="string">bss_faqs</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
